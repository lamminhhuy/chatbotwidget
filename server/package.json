{
  "name": "server",
  "version": "1.0.14",
  "description": "An Express boilerplate backend",
  "author": "Edwin Hernandez",
  "repository": "edwinhern/express-typescript-2024",
  "license": "MIT",
  "main": "index.ts",
"private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/main/index.ts | pino-pretty",
    "build": "tsup",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --fix",
    "format": "biome format src/",
    "test": "vitest run",
    "prepare": "husky"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "@google/generative-ai": "^0.21.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "axios": "^1.7.9",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.13.0",
    "pino-http": "^10.0.0",
    "redis": "^4.7.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "typeorm": "^0.3.20",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pino-pretty": "^11.0.0",
    "rimraf": "^6.0.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --apply --no-errors-on-unmatched"
    ]
  },
  "tsup": {
    "target": "esnext", 
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "resolutions": {
    "rollup": "4.24.0"
  },
  "type": "module"
}
